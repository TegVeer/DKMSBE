{
  "kind": "collectionType",
  "collectionName": "inventory_items",
  "info": {
    "singularName": "inventory-item",
    "pluralName": "inventory-items",
    "displayName": "Inventory Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "item_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "unit": {
      "type": "enumeration",
      "enum": [
        "kg",
        "gm",
        "carton",
        "box",
        "ltr",
        "bag",
        "pkt"
      ]
    },
    "stock_qty": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "threshold_qty": {
      "type": "integer"
    },
    "update_by": {
      "type": "string",
      "required": true
    },
    "exp_date": {
      "type": "string"
    },
    "last_update": {
      "type": "string",
      "required": true
    },
    "vendor": {
      "type": "string"
    }
  }
}
